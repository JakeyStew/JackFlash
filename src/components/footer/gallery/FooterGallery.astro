---
const { footer_images } = Astro.props;  // Get footer images directly from CMS

// Assuming `footer_images` is passed as part of your CMS configuration
const imagePaths = footer_images.map(image => image.src);  // Use the `src` or relevant key to access image URL
console.info(imagePaths); // Log paths for debugging
---

<div class="footer-gallery">
  {imagePaths.map((image) => (
    <img src={image} alt="Footer Gallery Image" class="footer-gallery-image" />
  ))}
</div>